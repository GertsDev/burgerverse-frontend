{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react-jsx",
    "types": ["node", "jest"],
    "baseUrl": "src",
    "paths": {
      "@api/*": ["utils/api/*"],
      "@components/*": ["components/*"],
      "@components": ["components/index.ts"],
      "@ui/*": ["components/ui/*"],
      "@ui": ["components/ui/index.ts"],
      "@pages/*": ["pages/*"],
      "@pages": ["pages/index.ts"],
      "@images/*": ["images/*"],
      "@ui-pages/*": ["components/ui/pages/*"],
      "@ui-pages": ["components/ui/pages/index.ts"],
      "@slices/*": ["services/slices/*"],
      "@selectors/*": ["services/selectors/*"],
      "@utils-types": ["utils/types.ts"],
      "@utils-cookie": ["utils/cookie.ts"],
      "@api-helpers": ["utils/api/helpers.ts"],
      "@redux-store": ["services/store.ts"],

    }
  },
  "include": ["src"]
}
